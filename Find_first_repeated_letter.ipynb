      "source": [
        "def find_first_repeated_letter_index(string):\n",
        "  for x in range(len(string)):\n",
        "\n",
        "    y = x + 1\n",
        "    while (y != len(string)):\n",
        "      if string [x] == string[y]:\n",
        "        return x\n",
        "      else:\n",
        "        y = y + 1\n",
        "  return -1"
      ]
